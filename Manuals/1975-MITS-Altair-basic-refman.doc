\par
\pard\plain\fi820\sl228\slmult1\pvpg\phpg\posx1178\posy9394\absh-1389\absw6332\overlay\s63\aspalpha\aspnum\faauto\adjustright\cgrid
{\cs19\lang1033\langfe1033\b0\i0\scaps0\strike0\cf1\expndtw0\charscalex100\dn0\fs15\afs15\ul0 50 PRINT "WHAT IS THE SECOND
 NUMBER";}
\par
\pard\plain\fi820\pvpg\phpg\posx1178\posy9394\absh-1389\absw6332\overlay\s63\aspalpha\aspnum\faauto\adjustright\cgrid
